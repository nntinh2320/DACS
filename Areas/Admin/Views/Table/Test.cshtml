@model IEnumerable<DACS.Models.ViTriLayMau>

@{
    ViewData["Title"] = "DongSong List";
}

<h1>DongSong List</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.DongSong.Id</td>
            </tr>
        }
    </tbody>
</table>
